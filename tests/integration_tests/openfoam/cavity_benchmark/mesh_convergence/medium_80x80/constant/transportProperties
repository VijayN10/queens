FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Transport model
transportModel  Newtonian;

// Kinematic viscosity for Re = 1000
// Re = U*L/nu, where U=1 m/s, L=1 m
// Therefore: nu = U*L/Re = 1/1000 = .00100000
nu              [0 2 -1 0 0 0 0] .00100000;

